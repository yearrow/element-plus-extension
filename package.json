{
  "name": "epe",
  "description": "element-plus扩展组件库",
  "scripts": {
    "build:theme": "pnpm --filter element-yearrow-theme run build",
    "dev:lib": "pnpm --filter element-plus-extension run dev",
    "build:lib": "pnpm --filter element-plus-extension run build",
    "dev:docs": "pnpm --filter document run dev",
    "start": "pnpm --filter document run start",
    "copy:theme": "cp -r packages/theme/dist/theme   packages/epe/lib/theme",
    "build": "pnpm run build:theme && pnpm run build:lib && npm run copy:theme && pnpm --filter document run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yearrow/element-plus-extension.git"
  },
  "author": "wangnamin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yearrow/element-plus-extension/issues"
  },
  "homepage": "https://github.com/yearrow/element-plus-extension#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "eslint": "^8.27.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.7.0"
  }
}
